javascript:(async()=>{const baseUrl=window.location.href.split('?')[0];const domains=[];const lastPageLink=Array.from(document.querySelectorAll('a')).find(a=>a.textContent.trim()==='Â»'&&a.href.includes('page='));const totalPages=lastPageLink?parseInt(lastPageLink.href.match(/page=(\d+)/)?.[1],10):1;for(let i=1;i<=totalPages;i++){const url=i===1?baseUrl:`${baseUrl}?page=${i}`;try{const res=await fetch(url,{credentials:'include'});const html=await res.text();const doc=new DOMParser().parseFromString(html,'text/html');const links=doc.querySelectorAll('tbody td a.link');links.forEach(link=>domains.push(link.textContent.trim()))}catch(e){console.error(`Error on page ${i}: ${e.message}`)}}setTimeout(()=>{document.open();document.write('<pre>'+domains.join('\n')+'</pre>');document.close();},1000);})();
